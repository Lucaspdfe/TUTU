{
    "configurations": [
        {
            "name": "GCC",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/toolchain/i686-elf/lib/gcc/i686-elf/14.2.0/include/**"
            ],
            "defines": [
                "TARGET=i686"
            ],
            "compilerPath": "${workspaceFolder}/toolchain/i686-elf/bin/i686-elf-gcc",
            "cStandard": "c99",
            "cppStandard": "c++14",
            "intelliSenseMode": "gcc-x86",
            "compilerArgs": [
                "-std=c99 -g -ffreestanding -nostdlib"
            ]
        },
        {
            "name": "KERNEL",
            "includePath": [
                "${workspaceFolder}/src/kernel/includes"
            ],
            "defines": [],
            "compilerPath": "${workspaceFolder}/toolchain/i686-elf/bin/i686-elf-g++",
            "cStandard": "c17",
            "cppStandard": "gnu++17",
            "intelliSenseMode": "gcc-x86",
            "compilerArgs": [
                "-ffreestanding",
                "-nostdlib",
                "-Iincludes"
            ]
        }
    ],
    "version": 4
}